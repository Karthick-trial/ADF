{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mukeshlr26-df"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS2'"
		},
		"LS3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS3'"
		},
		"LKforLK1_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaulttrial5.vault.azure.net/"
		},
		"LS1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretkeyvoult"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mukeshlr26-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS2_connectionString": {
						"value": "[parameters('LS2_connectionString')]"
					},
					"LS3_connectionString": {
						"value": "[parameters('LS3_connectionString')]"
					},
					"LKforLK1_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LKforLK1_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS1_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}